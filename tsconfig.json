{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "jsx": "react",
    "module": "es6",
    "target": "es5",
    "lib": [ "es5", "es6", "es2015.core", "dom" ],
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}
